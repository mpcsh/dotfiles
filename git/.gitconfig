	email = m@markpcohen.com
	name = Mark Cohen
	signingkey = 869DD7FD17A9933A2E7A464CF1DBC528097D76F1
[credential]
	helper = store
[push]
	default = simple
[alias]
	co = checkout
	ci = commit -m
	ca = commit -am
	st = status
	ls = log --oneline --decorate --graph --all -n 10
	la = log --oneline --decorate --graph --all
	hub = "!f() { if [[ $# == 1 ]]; then cd ~/projects; git clone https://github.com/$1.git; else echo "must supply a repo"; fi }; f"
	undo = "!f() { git reset --hard $(git rev-parse --abbrev-ref HEAD)@{${1-1}}; }; f"
[web]
	browser = xdg-open
