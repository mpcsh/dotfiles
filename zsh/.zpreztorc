# color output (auto set to 'no' on dumb terminals)
zstyle ':prezto:*:*' color 'yes'

# set the prezto modules to load - order matters
zstyle ':prezto:load' pmodule \
  'environment' \
  'terminal' \
  'history' \
  'completion' \
  'prompt' \
  'git' \
  'syntax-highlighting' \
  'autosuggestions'
