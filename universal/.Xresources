!bar
barHeight: 48
barOffset: 18
background: #222222
foreground: #c2c2b0

!font
Xft.dpi: 96
*font: xft:M+1mn:size=12
*barFont: xft:M+1mn:size=12
*iconFont: xft:FontAwesome:size=12

!font rendering
Xft.antialias: true
Xft.autohint: false
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.lcdfilter: none
Xft.rgba: rgba

! special
*.foreground:   #b5b5a4
*.background:   #222222

! black
*.color0:       #555555
*.color8:       #888888

! red
*.color1:       #9f4c56
*.color9:       #ff7a8a

! green
*.color2:       #709c49
*.color10:      #b7ff77

! yellow
*.color3:       #9d8c48
*.color11:      #ffe375

! blue
*.color4:       #4b769d
*.color12:      #7ac0ff

! magenta
*.color5:       #9a4d8d
*.color13:      #ff80e9

! cyan
*.color6:       #4d9a9a
*.color14:      #80ffff

! white
*.color7:       #bbbbbb
*.color15:      #eeeeee
